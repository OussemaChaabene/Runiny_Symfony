{% extends 'indexx.html.twig' %}

{% block title %}Abonnement index{% endblock %}

{% block body %}
    <h1>Vos Abonnements</h1>


    <table class="table">
        <thead>
            <tr>
                <th>nom</th>
                <th>Type</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for a in categorieabos %}
            <tr>
                <td>{{ a.nom}}</td>
                <td>{{ a.abonnement }}</td>

                <td>

                    <a href="{{ path('app_categorieabo_show', {'id': a.id}) }}">show</a>
                    <a href="{{ path('app_categorieabo_edit', {'id': a.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="">Rien a affichier</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_categorieabo_new') }}">Create new</a>
{% endblock %}
